{
  "name": "xor",
  "input": "int, int | uint, uint",
  "output": "int | uint",
  "snippet": "(xor $1 $2)",
  "description": [
    "Returns the result of bitwise exclusive or'ing <strong>$1</strong> with <strong>$2</strong>."
  ],
  "examples": [
    [">", "(xor 1 1)"],
    ["<", "0"],
    [">", "(xor 1 2)"],
    ["<", "3"],
    [">", "(xor u2 u1)"],
    ["<", "u3"],
    [">", "(xor -9 1)"],
    ["<", "-10"],
    [">", "(xor -8 -3)"],
    ["<", "5"],
    [">", "(xor 120 280)"],
    ["<", "352"]
  ]
}
