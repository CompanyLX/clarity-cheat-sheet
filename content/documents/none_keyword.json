  "byname": "",
  "inputs": "",
  "output": "(optional ?)",
  "since": 2.1,
  "description": [
    "Represents the <u>none</u> option indicating no value for a given optional (analogous to a null value)."
  ],
  "examples": [
    [">", "(define-public (only-if-positive (a int))"],
    [">", "(if (> a 0)"],
    [">", "(some a)"],
