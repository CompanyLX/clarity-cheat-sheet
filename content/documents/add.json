{
  "id": "add",
  "name": "+",
  "byname": "add",
  "input": "int, ... | uint, ...",
  "output": "int | uint",
  "signature": "(+ INPUT-1 INPUT-2 ...)",
  "description": [
    "Adds a variable number of integer inputs and returns the result. In the event of an <u>overflow</u> or <u>underflow</u>, throws a runtime error."
  ],
  "examples": [
    [">", "(+ 1 2 3)"],
    ["<", "6"],
    [">", "(+ 2 -1)"],
    ["<", "1"],
    [">", "(+ -3 -5 -7 -9)"],
    ["<", "-24"],
    [">", "(+ 1 2 3 4 5 6 7 8 9)"],
    ["<", "45"],
    [">", "(+ 0 0)"],
    ["<", "0"],
    [">", "(+ 5)"],
    ["<", "5"],
    [">", "(+ u31 u69)"],
    ["<", "100"],
    [";", "Attempt to add diffent types"],
    [">", "(+ u16 4)"],
    ["!", ""]
  ]
}
