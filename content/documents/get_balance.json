{
  "wip": true,
  "id": "get_balance",
  "name": "ft-get-balance",
  "byname": "",
  "input": "TokenName, principal",
  "output": "uint",
  "since": 2.1,
  "description": [
    "<i>ft-get-balance</i> returns <i>token-name</i> balance of the principal <i>principal</i>. The token type must have been defined using <i>define-fungible-token</i>."
  ],
  "signature": "(ft-get-balance token-name principal)",
  "examples": [
    [">", "(define-fungible-token stackaroo)"],
    [
      ">",
      "(ft-mint? stackaroo u100 'SZ2J6ZY48GV1EZ5V2V5RB9MP66SW86PYKKQ9H6DPR)"
    ],
    [
      ">",
      "(ft-get-balance stackaroo 'SZ2J6ZY48GV1EZ5V2V5RB9MP66SW86PYKKQ9H6DPR)"
    ],
    ["<", "u100"]
  ]
}
