{
  "name": "+",
  "byname": "mode",
  "input": "int, int | uint, uint",
  "output": "int | uint",
  "snippet": "(mod $1 $2)",
  "description": [
    "Returns the integer remainder from integer dividing <strong>$1</strong> by <strong>$2</strong>. In the event of a division by zero, throws a runtime error."
  ],
  "examples": [
    [">", "(mod 2 3)"],
    ["<", "2"],
    [">", "(mod 5 2)"],
    ["<", "1"],
    [">", "(mod 7 1)"],
    ["<", "0"],
    [">", "(mod -10 3)"],
    ["<", "-1"],
    [">", "(mod -8 -3)"],
    ["<", "-2"],
    [">", "(mod 5 0)"],
    ["!", ""]
  ]
}
