{
  "id": "_divide",
  "name": "/",
  "byname": "Divide",
  "inputs": [
    ["NUMBER-1", "int | uint"],
    ["NUMBER-2", "int | uint"],
    ["...", "int | uint"],
    ["NUMBER-N", "int | uint"]
  ],
  "output": "int | uint",
  "signature": "(/ NUMBER-1 NUMBER-2 ... NUMBER-N)",
  "description": [
    "Divides a variable number of integer inputs and returns the whole integer of the result. In the event of a <u>division by zero</u>, throws a runtime error."
  ],
  "examples": [
    [">", "(/ 2 3)"],
    ["<", "0"],
    [">", "(/ 5 2)"],
    ["<", "2"],
    [">", "(/ u10 u3)"],
    ["<", "u3"],
    [">", "(/ 4 2 2)"],
    ["<", "1"],
    [">", "(/ 0 9)"],
    ["<", "0"],
    [">", "(/ 9 0)"],
    ["!", ""],
    [";", "Attempt to divide diffent types"],
    [">", "(/ u16 4)"],
    ["!", ""]
  ]
}
