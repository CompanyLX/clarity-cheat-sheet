{
  "name": "int-to-utf8",
  "input": "int | uint",
  "output": "string-utf8",
  "snippet": "(int-to-utf8 $1)",
  "since": 2.1,
  "description": [
    "Converts an integer, either <strong>int</strong> or <strong>uint</stront>, to a <strong>string-utf8</strong> string-value representation."
  ],
  "examples": [
    [">", "(int-to-utf8 1)"],
    ["<", "u\"1\""],
    [">", "(int-to-utf8 u1)"],
    ["<", "u\"1\""],
    [">", "(int-to-utf8 -1)"],
    ["<", "u\"-1\""],
    [">", "(define-data-var size uint 64)"],
    [">", "(int-to-utf8 (var-set size))"],
    ["<", "u\"64\""]
  ]
}
