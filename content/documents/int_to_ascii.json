{
  "name": "int-to-ascii",
  "input": "int | uint",
  "output": "string-ascii",
  "snippet": "(int-to-ascii $1)",
  "since": 2.1,
  "description": [
    "Converts an integer, either <strong>int</strong> or <strong>uint</stront>, to a <strong>string-ascii</strong> string-value representation."
  ],
  "examples": [
    [">", "(int-to-ascii 1)"],
    ["<", "\"1\""],
    [">", "(int-to-ascii u1)"],
    ["<", "\"1\""],
    [">", "(int-to-ascii -1)"],
    ["<", "\"-1\""],
    [">", "(define-data-var size uint 64)"],
    [">", "(int-to-ascii (var-set size))"],
    ["<", "\"64\""]
  ]
}
