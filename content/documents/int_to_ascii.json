{
  "name": "int-to-ascii",
  "input": "int | uint",
  "output": "string-ascii",
  "snippet": "(int-to-ascii IN1)",
  "since": 2.1,
  "description": [
    "Converts an integer, either <strong>int</strong> or <strong>uint</stront>, to a <strong>string-ascii</strong> string-value representation."
  ],
  "examples": [
    {
      "command": "(int-to-ascii 1)"
    },
    {
      "output": "\"1\""
    },
    {
      "command": "(int-to-ascii u1)"
    },
    {
      "output": "\"1\""
    },
    {
      "command": "(int-to-ascii -1)"
    },
    {
      "output": "\"-1\""
    },
    {
      "command": "(define-data-var size uint 64)"
    },
    {
      "command": "(int-to-ascii (var-set size))"
    },
    {
      "output": "\"64\""
    }
  ]
}
