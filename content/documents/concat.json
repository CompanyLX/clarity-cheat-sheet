{
  "id": "concat",
  "name": "concat",
  "inputs": [
    ["SEQUENCE-VALUE-1", "buff | string-ascii | string-utf8 | list"],
    ["SEQUENCE-VALUE-2", "buff | string-ascii | string-utf8 | list"]
  ],
  "output": "buff | string-ascii | string-utf8 | list",
  "signature": "(concat SEQUENCE-VALUE-1 SEQUENCE-VALUE-2)",
  "description": [
    "The <i>concat</i> function takes two sequences of the same type, and returns a concatenated sequence of the same type."
  ],
  "examples": [
    [">", "(concat (list 1 2) (list 3 4))"],
    ["<", "(1 2 3 4)"],
    [">", "(concat \"hello \" \"world\")"],
    ["<", "hello world"],
    [">", "(concat 0x0102 0x0304)"],
    ["<", "0x01020304"]
  ]
}
