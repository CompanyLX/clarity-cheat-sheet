{
  "wip": true,
  "id": "contract_of",
  "name": "contract-of",
  "byname": "",
  "input": "Trait",
  "output": "principal",
  "signature": "",
  "since": 2.1,
  "description": [
    "The <i>contract-of</i> function returns the principal of the contract implementing the trait."
  ],
  "signature": "(contract-of .contract-name)",
  "examples": [
    [
      ">",
      "(use-trait token-a-trait 'SPAXYA5XS51713FDTQ8H94EJ4V579CXMTRNBZKSF.token-a.token-trait)"
    ],
    [
      ">",
      "(define-public (forward-get-balance (user principal) (contract <token-a-trait>))"
    ],
    [">", "(begin"],
    [">", "(ok (contract-of contract))))"],
    ["<", "the principal of the contract implementing <token-a-trait>"]
  ]
}
