{
  "id": "accessors_and_mutators",
  "name": "Accessors & Mutators",
  "entries": [
    {
      "id": "var_get",
      "name": "var-get",
      "input": "VARIABLE_NAME",
      "output": "ANY_VALUE",
      "signature": "(var-get VARIABLE_NAME)",
      "description": "The `var-get` function looks up and returns an entry from a contract's data map. The value is looked up using `var-name`.",
      "examples": [
        {
          "command": "(define-data-var index int 6)"
        },
        {
          "command": "(var-get index)",
          "output": "6"
        },
        {
          "command": "(define-data-var message string-ascii \"Hello World!\")"
        },
        {
          "command": "(var-get message)",
          "output": "Hello World!"
        }
      ]
    },
    {
      "id": "var_set",
      "name": "var-set",
      "input": "VARIABLE_NAME, ANY_VALUE",
      "output": "bool",
      "signature": "(var-set VARIABLE_NAME, ANY_VALUE)",
      "description": "The `var-set` function sets the value associated with the input variable to the inputted value. The function always returns `true`.",
      "examples": [
        {
          "command": "(define-data-var index int 6)"
        },
        {
          "command": "(var-get index)",
          "output": "6"
        },
        {
          "command": "(var-set index 10)",
          "output": "true"
        },
        {
          "command": "(var-get index)",
          "output": "10"
        },
        {
          "command": "(define-data-var message string-ascii \"Hello World!\")"
        },
        {
          "command": "(var-get message)",
          "output": "Hello World!"
        },
        {
          "command": "(var-set message \"Go World, Go!\")",
          "output": "true"
        },
        {
          "command": "(var-get message)",
          "output": "Go World, Go!"
        }
      ]
    }
  ]
}
